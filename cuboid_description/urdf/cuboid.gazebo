<?xml version="1.0" ?>
<robot name="cuboid" xmlns:xacro="http://www.ros.org/wiki/xacro" >

<xacro:property name="cuboid_color" value="Gazebo/Orange" />

<gazebo reference="cuboid">
  <material>${cuboid_color}</material>
  <mu1>1.0</mu1>
  <mu2>1.0</mu2>
  <selfCollide>true</selfCollide>
  <gravity>true</gravity>
</gazebo>

</robot>
